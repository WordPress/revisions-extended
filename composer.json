{
	"name": "wporg/revisions-extended",
	"description": "Extended functionality for the WordPress revision system.",
	"homepage": "https://wordpress.org",
	"license": "GPL-2.0-or-later",
	"support": {
		"issues": "https://github.com/coreymckrill/revisions-extended/issues"
	},
	"config": {
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/WordPress/wporg-repo-tools"
		}
	],
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7",
		"wp-coding-standards/wpcs": "2.*",
		"phpcompatibility/phpcompatibility-wp": "*",
		"phpunit/phpunit": "7.5.20",
		"wp-phpunit/wp-phpunit": "^5.9",
		"wporg/wporg-repo-tools": "dev-trunk"
	},
	"scripts": {
		"format": "phpcbf -p",
		"lint": "phpcs"
	}
}
